#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_fast-xml-parser@4.0.11@fast-xml-parser/src/cli/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_fast-xml-parser@4.0.11@fast-xml-parser/src/cli/cli.js" "$@"
  ret=$?
fi
exit $ret
